      "camelcase": "XCompose",
      "title": "XCompose",
      "scope": "text.xcompose",
      "file-types": [
        ".XCompose",
        "XCompose",
        "xcompose"
      ],
      "class-name": "TreeSitterXCompose",
      "injection-regex": "^(XC|xc)ompose$",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
